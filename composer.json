{
    "name": "amphp/rpc",
    "description": "Remote procedure calls for PHP based on Amp.",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "amphp/amp": "^3",
        "amphp/byte-stream": "^2",
        "amphp/http-server": "3.x-dev",
        "amphp/http-client": "5.x-dev",
        "amphp/serialization": "^1.0",
        "amphp/sync": "^2",
        "ocramius/proxy-manager": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^9.1",
        "amphp/log": "^v2.0.0-beta.2",
        "amphp/phpunit-util": "^3",
        "amphp/php-cs-fixer-config": "^2-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amp\\Rpc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Rpc\\Examples\\Basic\\": "examples/src"
        },
        "files": [
            "examples/src/functions.php"
        ]
    }
}
