{
    "name": "amphp/rpc",
    "description": "Remote procedure calls for PHP based on Amp.",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "amphp/amp": "^2.4",
        "amphp/byte-stream": "^1.7",
        "amphp/http-server": "^2.0",
        "amphp/http-client": "^4.2",
        "amphp/serialization": "^1.0",
        "amphp/sync": "^1.3",
        "friendsofphp/proxy-manager-lts": "^1.0.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^9.1",
        "amphp/log": "^1.1",
        "amphp/phpunit-util": "^2",
        "amphp/php-cs-fixer-config": "^2-dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amp\\Rpc\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amp\\Rpc\\Examples\\Basic\\": "examples/src"
        },
        "files": [
            "examples/src/functions.php"
        ]
    }
}
